{
  "name": "gongbushang.nuxt",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register -P ./tsconfig.server.json src/main.ts",
    "start:dev": "DEV_MODE=server nodemon",
    "prestart:prod": "rm -rf server && tsc -P ./tsconfig.server.json",
    "start:prod": "node server/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "three:build": "gulp -f gulpfiles/three.js",
    "nuxt:gen": "NODE_ENV=production nuxt generate -c src/nuxt.config.js",
    "pack": "rm -f bundle.tgz && node builder/tar.js",
    "prd": "npm run nuxt:gen &&  npm run prestart:prod && npm run three:build && npm run pack",
    "sync": "sh sync.sh"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/graphql": "^3.0.0",
    "@nuxtjs/apollo": "^4.0.0-beta.6",
    "@nuxtjs/vuetify": "^0.4.2",
    "@types/mongoose": "^5.2.0",
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "dev": "^0.1.3",
    "express": "^4.16.3",
    "fastify-formbody": "^2.0.0",
    "fs-extra": "^6.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.5",
    "lodash": "^4.17.10",
    "md5": "^2.2.1",
    "mongodb": "^3.1.1",
    "morgan": "^1.9.0",
    "nuxt-class-component": "^1.2.1",
    "nuxt-edge": "^2.0.0-25506422.d9d2d9e",
    "nuxt-typescript": "^0.11.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "three-orbitcontrols": "^2.1.2",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.6.2",
    "vue": "^2.5.16",
    "vue-property-decorator": "^7.0.0",
    "vuetify": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "@types/three": "^0.92.12",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-uglify": "^3.0.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "tar": "^4.4.4",
    "three": "^0.94.0",
    "three-fbx-loader": "^1.0.3",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "tslint-react": "^3.6.0",
    "vuex-class": "^0.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
