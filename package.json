{
  "name": "gongbushang.com",
  "version": "1.0.0",
  "description": "My cool Nuxt.js project",
  "author": "gongbaodd",
  "private": true,
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.js(x)|*.json|*.ts(x)|*.markdown|*.md|*.vue": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "emoji-cz"
    }
  },
  "dependencies": {
    "@bazzite/nuxt-optimized-images": "0.3.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/google-analytics": "2.2.3",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "@nuxtjs/sentry": "3.3.1",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "nuxt": "2.12.0",
    "nuxt-class-component": "1.3.0",
    "nuxt-helmet": "1.2.3",
    "vue-class-component": "7.2.3",
    "vue-property-decorator": "8.4.1",
    "vuex-class": "0.3.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "0.6.0",
    "@nuxtjs/eslint-config": "2.0.2",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/vuetify": "1.11.0",
    "@types/node": "13.9.3",
    "@vue/test-utils": "1.0.0-beta.32",
    "audit-ci": "2.5.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "commitizen": "4.0.3",
    "emoji-cz": "0.3.1",
    "es-check": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-nuxt": "0.5.1",
    "eslint-plugin-prettier": "3.1.2",
    "gh-pages": "2.2.0",
    "husky": "4.2.3",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0",
    "imagemin-svgo": "7.1.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.8",
    "prettier": "2.0.1",
    "ts-node": "8.7.0",
    "vue-jest": "4.0.0-beta.2"
  },
  "scripts": {
    "dev": "nuxt -c nuxt.config.js",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate:prd": "NODE_ENV=production nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "echo 'text'",
    "cy:test": "sh scripts/run_cypress.sh",
    "commit": "git-cz",
    "cp:cname": "sh ./scripts/cp_cname.sh",
    "deploy:coding": "sh ./scripts/deploy_to_coding.sh",
    "es:check": "es-check",
    "deploy": "gh-pages -d www -r https://$GH_TOKEN@github.com/gongbaodd/gongbushang.com.git -b master"
  }
}
